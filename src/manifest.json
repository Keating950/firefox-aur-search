{
    "manifest_version": 2,
    "name": "Arch Linux AUR Search",
    "version": "2.0.1",
    "description": "Search Arch User Repository packages for Arch Linux",
    "icons": {
        "64": "icons/archlinux-icon-crystal-64.svg"
    },
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "AUR packages",
            "search_url": "https://aur.archlinux.org/packages/?K={searchTerms}&SB=p&SO=d",
            "favicon_url": "https://aur.archlinux.org/images/favicon.ico",
            "keyword": "aur"
        }
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{83bd11d4-17f9-436c-82fc-d460bb28e8e1}",
            "strict_min_version": "55.0"
        }
    }
}
